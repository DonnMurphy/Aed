<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jambons.aed">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cryptids_logo_v2"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/cryptids_logo_v2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateNewAuctionActivity"></activity>
        <activity android:name=".ViewAuction" />
        <activity android:name=".ViewAuctions" />
        <activity android:name=".ViewSheep" />
        <activity android:name=".AccountUpdate" />
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewAllSheep" />
        <activity android:name=".AccountView" />
        <activity android:name=".ScanMenu" />
        <activity android:name=".ViewDeck" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>